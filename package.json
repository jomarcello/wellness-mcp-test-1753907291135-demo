{
  "name": "healthcare-ai-voice-agent-demo",
  "version": "1.0.0",
  "private": true,
  "description": "Scalable multi-tenant AI voice assistant platform designed to support hundreds of healthcare practices for lead generation and practice onboarding",
  "author": "Healthcare AI Demo Team",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "start:all": "pm2 start ecosystem.config.js",
    "start:spinealign": "pm2 start ecosystem.config.js --only spinealign-server,spinealign-tunnel",
    "start:smith": "pm2 start ecosystem.config.js --only smith-server,smith-tunnel",
    "stop:all": "pm2 stop all",
    "restart:all": "pm2 restart all",
    "status": "pm2 status",
    "logs": "pm2 logs --lines 50",
    "logs:spinealign": "pm2 logs spinealign-server spinealign-tunnel --lines 20",
    "logs:smith": "pm2 logs smith-server smith-tunnel --lines 20",
    "monit": "pm2 monit",
    "tunnel:status": "pm2 logs spinealign-tunnel smith-tunnel --lines 5",
    "health:check": "pm2 status && echo '\\n=== Tunnel URLs ===' && pm2 logs spinealign-tunnel --lines 5 | grep -E 'your url is|https://' || echo 'SpineAlign tunnel starting...' && pm2 logs smith-tunnel --lines 5 | grep -E 'your url is|https://' || echo 'Smith tunnel starting...'",
    "deploy:single": "node scripts/trigger-mass-deployment.js single",
    "deploy:all": "node scripts/trigger-mass-deployment.js all",
    "deploy:mass": "bash scripts/trigger-deployment.sh all",
    "generate:domain": "node scripts/generate-railway-domain.js",
    "railway:setup": "node scripts/manual-railway-setup.js",
    "railway:instructions": "node scripts/manual-railway-setup.js all",
    "railway:deploy": "./scripts/railway-cli-deploy.sh"
  },
  "keywords": [
    "healthcare",
    "ai",
    "voice-assistant",
    "nextjs",
    "elevenlabs",
    "openai",
    "multi-tenant",
    "practice-management",
    "lead-generation",
    "saas-platform",
    "scalable-architecture",
    "healthcare-saas",
    "practice-onboarding",
    "white-label",
    "automation",
    "tunnel-management"
  ],
  "dependencies": {
    "@google-cloud/speech": "^7.2.0",
    "@google-cloud/text-to-speech": "^6.2.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@slack/bolt": "^4.0.0",
    "@vapi-ai/web": "^2.2.0",
    "axios": "^1.9.0",
    "dotenv": "^16.0.0",
    "lucide-react": "^0.513.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.45.0",
    "next": "15.3.3",
    "openai": "^5.2.0",
    "playht": "^0.21.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jomarcello/Agentsdemo.git"
  },
  "license": "MIT"
}
